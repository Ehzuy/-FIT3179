{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "./database.csv"
  },

  "mark": {
    "type": "bar"
  },
  "encoding": {
    "x": {
      "field": "sex",
      "type":"nominal"
    },
    "y": {
      "aggregate": "count",
      "type":"quantitive"
    },
    "color": {
      "type": "nominal",
      "scale": {
        "domain": [
          "Male",
          "Female",
        ],
        "range": [
          "#1f77b4",
          "#9467bd"
        ]
    }
    }
  },
  "config": {}
}
