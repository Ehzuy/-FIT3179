{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "./database.csv"
  },
 
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "sex",
      "type": "nominal"
    },
    "y": {
      "aggregate": "count",

    },
    "color": {
      "type": "nominal",
      "field": "occupation",
      "title": "Weather Type", 
      "legend": {
        "orient": "left"
      }
    }
  },
  "config": {}
}
